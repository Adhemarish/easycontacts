function insertToNotes() {
  console.log('appel display')
  const tagsZone = document.getElementById('tag-search')
  tagsZone.style.display = "none";
  const notesList = document.getElementById('tag-notes-results')
  notesList.style.display= ""
  notesList.insertAdjacentHTML('beforeend', '<%= j render "tagnotes", notes: @notes, search: @search, tag_labels: @tag_labels %>')
}

insertToNotes()




