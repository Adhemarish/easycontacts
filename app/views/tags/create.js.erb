function insertToCheckbox() {
  const checkboxList = document.getElementById('tagsCheckboxes')
  checkboxList.insertAdjacentHTML('beforeend', '<%= j render "tags/checkbox", tag: @tag, checked: "checked" %>')
  //console.log(checkboxList.lastChild)
}

function refreshForm(innerHTML) {
  const newReviewForm = document.getElementById('tag_label')
  newReviewForm.value = ""
}

insertToCheckbox()
refreshForm()






